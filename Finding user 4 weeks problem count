#include <iostream>
using namespace std;
//Finding in how many weeks out of x weeks the programer has attempted more than 10 problems 
int main() {
    int x,prac[x],n=0;
    cin >> x;
    for(int i=0;i<x;i++){
        cin >> prac[i];
        if(prac[i]>=10)
        n++;
    }
    cout << n;
	return 0;
}
