#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--){
        int a,b,c,d=0,arr[10]={6,2,5,5,4,5,6,3,7,6};
        cin >> a >> b;
        c=a+b;
        while(c>0){
            int res=c%10;
            d+=arr[res];
            c=c/10;
        }
    cout << d << "\n";
    }
	return 0;
}
