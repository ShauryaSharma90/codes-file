#include <iostream>
using namespace std;
int main() {
    int t;
    cin >> t;
    while(t--){
        int b,c=0;
        cin >> b;
        int a[b];
        for(int i=0;i<b;i++){
            cin >> a[i];
            if(a[i]>=1000)
            c++;
        }
        cout << c << "\n";
    }
    
    return 0;
}
